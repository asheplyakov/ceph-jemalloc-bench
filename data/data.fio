[global]
rw=randwrite
bs=4k
runtime=7200
write_bw_log=data_bw.log
write_iops_log=data_iops.log
direct=1
ioengine=libaio
time_based
group_reporting

[sdz]
iodepth=8
filename=/dev/sdz1

[sdy]
iodepth=8
filename=/dev/sdy1

[sdx]
iodepth=8
filename=/dev/sdx1

[sdw]
iodepth=8
filename=/dev/sdw1

[sdv]
iodepth=8
filename=/dev/sdv1

[sdu]
iodepth=8
filename=/dev/sdu1
