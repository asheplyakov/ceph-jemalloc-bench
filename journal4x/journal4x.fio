[global]
bs=4k
ioengine=libaio
rw=write
fdatasync=1
iodepth=1
direct=1
runtime=3600
time_based
write_lat_log=journal_4x_lat.log
write_iops_log=journal_4x_iops.log
write_bw_log=journal_4x_bw.log


[journal1]
filename=/dev/sdf1

[journal2]
filename=/dev/sdf2

[journal3]
filename=/dev/sdf3

[journal4]
filename=/dev/sdf4
